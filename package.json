    "postinstall": "./scripts/postinstall.sh",
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "storybook:components": "nx storybook alpha-ui-component",
    "storybook:antd": "nx storybook alpha-ui-antd",
    "generate:graphql": "nx build tools-generators-graphql --skip-nx-cache",
    "generate:json": "nx build tools-generators-json-schema --skip-nx-cache",
    "commit": "git-cz",
    "docs": "onchange 'libs/ddd/**/*.ts' 'typedoc.json' -- npx typedoc",
    "prisma:reset": "npx prisma migrate reset --preview-feature -f --skip-generate",
    "prisma:sync": "npx prisma migrate dev --name init --preview-feature",
    "migration:generate": "nx migrate @nrwl/workspace@latest",
    "migration:run": "nx migrate --run-migrations=migrations.json",
    "docker:build": "./scripts/docker/build.sh",
    "docker:up": "./scripts/docker/up.sh",
    "docker:push": "./scripts/docker/push.sh",
    "start:prod:web": "pm2-runtime dist/apps/web/pm2.json",
    "start:prod:api-graph": "pm2-runtime dist/apps/api/graph/pm2.json",
    "generate-antd-json-schema": "ts-node ./json-schema-generator/json-schema-generator.ts",
    "codegen": "yarn cross-env DOTENV_CONFIG_PATH=.env graphql-codegen --require dotenv/config --config .graphqlconfig.yaml"
  },
