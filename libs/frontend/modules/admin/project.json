{
  "root": "libs/frontend/modules/admin",
  "sourceRoot": "libs/frontend/modules/admin/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/frontend/modules/admin/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"],
      "configurations": {
        "ci": {
          "eslintConfig": "libs/frontend/modules/admin/.eslintrc.ci.json"
        }
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/frontend/modules/admin"],
      "options": {
        "jestConfig": "libs/frontend/modules/admin/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["layer:domain:frontend", "scope:frontend"]
}
