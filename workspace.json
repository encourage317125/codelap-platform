{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "web",
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "css"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {}
    }
  },
  "projects": {
    "tools-eslint-config-codelab": {
      "root": "libs/tools/eslint-config-codelab",
      "sourceRoot": "libs/tools/eslint-config-codelab/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tools/eslint-config-codelab/**/*.ts",
              "libs/tools/eslint-config-codelab/**/*.yaml",
              "libs/tools/eslint-config-codelab/**/*.spec.ts",
              "libs/tools/eslint-config-codelab/**/*.spec.tsx",
              "libs/tools/eslint-config-codelab/**/*.spec.js",
              "libs/tools/eslint-config-codelab/**/*.spec.jsx",
              "libs/tools/eslint-config-codelab/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tools/eslint-config-codelab/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/tools/eslint-config-codelab"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/tools/eslint-config-codelab",
            "tsConfig": "libs/tools/eslint-config-codelab/tsconfig.lib.json",
            "packageJson": "libs/tools/eslint-config-codelab/package.json",
            "main": "libs/tools/eslint-config-codelab/src/index.ts",
            "assets": ["libs/tools/eslint-config-codelab/**/.*.yaml"]
          },
          "outputs": ["{options.outputPath}"]
        }
      }
    },
    "web": {
      "root": "apps/web",
      "sourceRoot": "apps/web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build-next": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/web",
            "outputPath": "dist/apps/web",
            "assets": [
              {
                "glob": "pm2.json",
                "input": "apps/web",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {}
          },
          "outputs": ["{options.outputPath}"]
        },
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/web",
            "outputPath": "dist/apps/web"
          },
          "configurations": {
            "production": {}
          }
        },
        "build-prod": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn cross-env NODE_ENV=production nx build-next web --prod",
              "yarn copyfiles -f apps/web/pm2.json dist/apps/web"
            ],
            "parallel": false
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "web:build-next",
            "dev": true,
            "port": 3000,
            "maxWorkers": 4,
            "memoryLimit": 2048
          },
          "configurations": {
            "ci": {
              "port": 3001
            },
            "production": {
              "buildTarget": "web:build-next:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "web:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web/**/*.ts",
              "apps/web/**/*.tsx",
              "apps/web/next-env.d.ts",
              "apps/web/**/*.spec.ts",
              "apps/web/**/*.spec.tsx",
              "apps/web/**/*.spec.js",
              "apps/web/**/*.spec.jsx",
              "apps/web/**/*.d.ts",
              "apps/src/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/web"]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/web",
            "config": {
              "configFolder": "apps/web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          },
          "outputs": ["{options.outputPath}"]
        }
      }
    },
    "web-e2e": {
      "root": "apps/web-e2e",
      "sourceRoot": "apps/web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-e2e/cypress.json",
            "tsConfig": "apps/web-e2e/tsconfig.e2e.json",
            "baseUrl": "http://localhost:3000"
          },
          "configurations": {
            "ci": {
              "baseUrl": "http://localhost:3001",
              "headless": true
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/test-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
